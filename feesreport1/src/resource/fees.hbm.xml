<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="beans.Studentreg" table="student" schema="fee_report">
    <id name="rollno" column="sroll"/>
    <property name="name" column="sname"></property>
    <property name="fname" column="sfname"></property>
    <property name="dob" column="sdob"></property>
    <property name="email" column="semail"></property>
    <property name="mob" column="smob"></property>
    
    <property name="gender" column="sgender"></property>
    <property name="pinno" column="spin"></property>
    <property name="addr" column="sadd"></property>
    <property name="course" column="course"></property>
    
    </class>
    </hibernate-mapping>